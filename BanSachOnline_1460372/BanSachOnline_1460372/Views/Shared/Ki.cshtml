@model IEnumerable<BanSachOnlineConnection.Series>

<div class="row">
    @*@foreach (var item in Model.Take(6))*@
    @foreach (var item in Model)
    {
        <div class="col-sm-4 col-lg-4 col-md-4">
            <div class="thumbnail">

                <a href='@Url.Action("Series", "Product", new { id = item.ID_Series })'>
                    <img src='@Url.Content(@item.TrangBia_Series)' />
                </a>

                <div class="caption">
                    <h4 class="pull-right">$@item.Gia_Series</h4>
                    <h4>
                        <a>@Html.ActionLink(item.Ten_Series, "Series", new { id = item.ID_Series })</a>
                    </h4>
                    <p>@item.NoiDung_Series</p>
                </div>

            </div>
        </div>
    }
</div>


